{
  "args": [
    "--zk={{MINIMESOS_ZOOKEEPER}}",
    "--consul=1",
    "--consul-ip={{MINIMESOS_CONSUL_IP}}"
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "network": "BRIDGE",
      "image": "containersol/mesos-consul:latest"
    }
